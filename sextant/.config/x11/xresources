!!! Base16 Default Dark

#define FOREGROUND     #d0d0d0
#define BACKGROUND     #151515
#define CURSOR_COLOR   #c0c5ce

#define NORMAL_BLACK   #151515
#define NORMAL_RED     #ac4142
#define NORMAL_YELLOW  #90a959
#define NORMAL_GREEN   #f4bf75
#define NORMAL_BLUE    #6a9fb5
#define NORMAL_MAGENTA #aa759f
#define NORMAL_CYAN    #75b5aa
#define NORMAL_WHITE   #d0d0d0
#define BRIGHT_BLACK   #505050
#define BRIGHT_RED     #ac4142
#define BRIGHT_YELLOW  #90a959
#define BRIGHT_GREEN   #f4bf75
#define BRIGHT_BLUE    #6a9fb5
#define BRIGHT_MAGENTA #aa759f
#define BRIGHT_CYAN    #75b5aa
#define BRIGHT_WHITE   #f5f5f5

!!!! st Configuration

st.font:        Terminus:pixelsize=16:antialias=true:autohint=true;
